#: data/ui/window.blp:23
msgid "Main menu"
msgstr "Menú principal"

#: data/ui/window.blp:24
msgid "Open application menu with preferences and about options"
msgstr "Abrir menú de aplicación con preferencias y opciones de información"

#: data/ui/window.blp:43
msgid "Drop an Image"
msgstr "Arrastra una Imagen"

#: data/ui/window.blp:44
msgid "Drag and drop an image file here to view its metadata"
msgstr "Arrastra y suelta un archivo de imagen aquí para ver sus metadatos"

#: data/ui/window.blp:50
msgid "Drop zone for images"
msgstr "Zona de arrastre para imágenes"

#: data/ui/window.blp:51
msgid "Drag and drop an image file here to view its metadata. Supports JPEG and TIFF formats."
msgstr "Arrastra y suelta un archivo de imagen aquí para ver sus metadatos. Soporta formatos JPEG y TIFF."

#: data/ui/window.blp:61
msgid "Image preview"
msgstr "Vista previa de imagen"

#: data/ui/window.blp:62
msgid "Preview of the loaded image"
msgstr "Vista previa de la imagen cargada"

#: data/ui/window.blp:76
msgid "Metadata"
msgstr "Metadatos"

#: data/ui/window.blp:77
msgid "Image metadata information"
msgstr "Información de metadatos de imagen"

#: data/ui/window.blp:85
msgid "Metadata scroll area"
msgstr "Área de desplazamiento de metadatos"

#: data/ui/window.blp:92
msgid "Image metadata list"
msgstr "Lista de metadatos de imagen"

#: data/ui/window.blp:93
msgid "List of metadata extracted from the image including EXIF, GPS, and file information"
msgstr "Lista de metadatos extraídos de la imagen incluyendo información EXIF, GPS y del archivo"

#: data/ui/window.blp:100
msgid "Save Clean Copy"
msgstr "Guardar Copia Limpia"

#: data/ui/window.blp:106
msgid "Save a copy of the image with all metadata removed. Use Ctrl+S as keyboard shortcut."
msgstr "Guardar una copia de la imagen con todos los metadatos eliminados. Usa Ctrl+S como atajo de teclado."

#: data/ui/window.blp:108
msgid "Save image without metadata (Ctrl+S)"
msgstr "Guardar imagen sin metadatos (Ctrl+S)"

#: data/ui/window.blp:121
msgid "_Preferences"
msgstr "_Preferencias"

#: data/ui/window.blp:127
msgid "_About Scramble"
